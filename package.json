{
  "name": "papi-starter",
  "version": "0.0.1",
  "description": "Pointhub API Starter",
  "main": "build/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "check-types": "tsc --noemit -p tsconfig.build.json",
    "dev:compile": "tsc --preserveWatchOutput -w -p tsconfig.build.json",
    "dev:resolve-path": "tsc-alias -w -p tsconfig.build.json",
    "dev:run": "nodemon build/index.js",
    "dev": "npm run build:compile && npm run build:resolve-path && concurrently -k -p 'none' npm:dev:*",
    "build:compile": "tsc -p tsconfig.build.json",
    "build:resolve-path": "tsc-alias -p tsconfig.build.json",
    "build": "rimraf build && npm run build:compile && npm run build:resolve-path",
    "start": "node build/index.js",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --runInBand --detectOpenHandles --forceExit",
    "test:coverage": "rimraf coverage && npm run test -- --coverage",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [
    "pointhub",
    "api",
    "starter"
  ],
  "author": "Martien Dermawan Tanama <martiendt@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@point-hub/express-cli": "^0.2.2",
    "@point-hub/express-error-handler": "^0.0.6",
    "@point-hub/express-utils": "^0.1.1",
    "@point-hub/papi": "^0.0.4",
    "argon2": "^0.30.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.1.0",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@faker-js/faker": "^7.6.0",
    "@point-hub/express-factory": "^1.1.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.15.10",
    "@types/supertest": "^2.0.12",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "babel-jest": "^29.5.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "supertest": "^6.3.3",
    "tsc-alias": "^1.8.4",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=16.14"
  }
}
